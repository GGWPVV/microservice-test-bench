{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: dj_example",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "runserver"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/bases/backend/dj_example/manage.py",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/bases:${workspaceFolder}/components:${workspaceFolder}/development"
      },
      "python": "${workspaceFolder}/.venv/bin/python"
    },
    {
      "name": "Python Current File FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "fastapi",
      "args": [
        "dev",
        "${file}"
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/bases:${workspaceFolder}/components:${workspaceFolder}/development"
      },
      "python": "${workspaceFolder}/.venv/bin/python"
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/bases:${workspaceFolder}/components:${workspaceFolder}/development"
      }
    }
  ]
}